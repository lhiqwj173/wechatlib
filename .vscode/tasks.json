{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "BUILD DLL",
			"command": "cl.exe",
			"args": [
				"/LD",
				"/arch:IA32",
				"/EHsc",
				"/Od",
				"/Zi",
				"/I${workspaceFolder}\\wechatlib\\src",
				"/IC:\\Program Files (x86)\\Windows Kits\\10\\Include\\10.0.22621.0\\um",
				"/D_CRT_SECURE_NO_WARNINGS",
				"/D_MBCS",
				"/Fo:${workspaceFolder}\\wechatlib\\build\\",
				"/Fe:${workspaceFolder}\\wechatlib\\Release\\wechat.dll",
				"${workspaceFolder}\\wechatlib\\src\\*.cpp",
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"detail": "编译器: cl.exe"
		},
		{
			"type": "cppbuild",
			"label": "BUILD TEST",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/EHsc",
				"/nologo",
				"/IC:\\Program Files (x86)\\Windows Kits\\10\\Include\\10.0.22621.0\\um",
				"/Fo:${workspaceFolder}\\wechatlib\\build\\",
				"/Fe:${workspaceFolder}\\wechatlib\\Release\\wechat_test.exe",
				"${workspaceFolder}\\wechatlib\\src\\*.cpp",
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"detail": "编译器: cl.exe"
		},
		{
			"type": "cppbuild",
			"label": "MSVC",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/EHsc",
				"/nologo",
				"/IC:\\Program Files (x86)\\Windows Kits\\10\\Include\\10.0.22621.0\\um",
				"/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
				"${workspaceFolder}\\wechatlib\\src\\wechat.cpp",
				"${file}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"detail": "编译器: cl.exe"
		}
	]
}